class PalindromeOfString {
    public static void main(String[] args) {
    String s="malayalam";
    String rev="";
    int len=s.length();
    for(int i=len-1;i>=0;i--){
        rev=rev+s.charAt(i);
    }
     System.out.println("Given String is palindrome!");
      System.out.println(rev);
      }
    }
