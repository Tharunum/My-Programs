class FibonacciSeries {                     //fibonacci series-->starts from 0 & 1 & next digit is sum of previous 2 numbers
    public static void main(String[] args) {
   int n=10;
   int a=0,b=1;
   for(int i=2;i<=n;i++){
       int c=a+b;
       System.out.println(c);
       a=b;
       b=c;
   }
      }
    }
